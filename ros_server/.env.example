# ROS Mock Server Configuration

# Server port (default: 4000)
PORT=4000

# Environment
NODE_ENV=development

# Optional: Simulate response delays (in milliseconds)
MIN_PROCESSING_DELAY=200
MAX_PROCESSING_DELAY=800

# Optional: Error simulation rate (0.0 to 1.0, where 1.0 = 100% errors)
ERROR_SIMULATION_RATE=0.0

# Optional: Default driver capacity limits
DEFAULT_DRIVER_CAPACITY=100

# Optional: Average speed for ETA calculations (km/h)
AVERAGE_SPEED_KMH=30